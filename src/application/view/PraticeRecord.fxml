<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="anchor" prefHeight="400.0" prefWidth="600.0" stylesheets="@../application.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.view.PraticeRecordController">
   <children>
      <Button id="quitbutton" layoutX="14.0" layoutY="360.0" mnemonicParsing="false" onAction="#changeSceneToMenu" stylesheets="@application.css" text="Quit">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <AnchorPane id="menuscheme1" layoutX="-2.0" layoutY="36.0" prefHeight="289.0" prefWidth="606.0" stylesheets="@application.css">
         <children>
            <Label id="menuscheme1" fx:id="numLabel" alignment="CENTER" layoutY="14.0" prefHeight="183.0" prefWidth="600.0" text="hi" textAlignment="CENTER" textFill="#bdc7c1">
               <font>
                  <Font size="96.0" />
               </font>
            </Label>
            <ButtonBar layoutX="72.0" layoutY="182.0" prefHeight="46.0" prefWidth="444.0">
              <buttons>
                <Button id="menuscheme2" fx:id="listenButton" mnemonicParsing="false" onAction="#listenRecording" prefHeight="46.0" text="Listen">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Button>
                  <Button id="menuscheme2" mnemonicParsing="false" onAction="#record" prefHeight="64.0" prefWidth="270.0" text="Record">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Button>
                  <Button id="menuscheme2" fx:id="submitButton" mnemonicParsing="false" onAction="#submit" prefHeight="88.0" text="Submit">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Button>
              </buttons>
            </ButtonBar>
            <ProgressBar fx:id="pb" layoutX="95.0" layoutY="245.0" prefHeight="20.0" prefWidth="419.0" progress="0.0" />
         </children>
         <effect>
            <DropShadow />
         </effect>
      </AnchorPane>
   </children>
</AnchorPane>
