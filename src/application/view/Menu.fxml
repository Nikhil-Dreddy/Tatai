<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="easyButton" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.view.MenuController">
   <children>
      <Label layoutX="13.0" layoutY="27.0" prefHeight="107.0" prefWidth="573.0" text="Welcome to Tatai!" textAlignment="CENTER">
         <font>
            <Font size="62.0" />
         </font>
      </Label>
      <Label layoutX="86.0" layoutY="121.0" prefHeight="107.0" prefWidth="426.0" text="Choose a level to play:" textAlignment="CENTER">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Button layoutX="110.0" layoutY="252.0" mnemonicParsing="false" onAction="#easyButtonClicked" prefHeight="68.0" prefWidth="145.0" text="Easy">
         <font>
            <Font size="36.0" />
         </font>
      </Button>
      <Button layoutX="359.0" layoutY="252.0" mnemonicParsing="false" onAction="#hardButtonClicked" prefHeight="68.0" prefWidth="145.0" text="Hard">
         <font>
            <Font size="36.0" />
         </font>
      </Button>
     <Button layoutX="13.0" layoutY="355.0" mnemonicParsing="false" onAction="#changeSceneToInstructions" text="Instructions">
        <font>
            <Font size="19.0" />
          </font></Button>
      <Button layoutX="261.0" layoutY="336.0" mnemonicParsing="false" onAction="#changeSceneToScore" text="Scores">
         <font>
            <Font size="19.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
